        <repository>
            <id>cspace.local.nuxeo</id>
            <url>${nuxeo.local.repo.dir}</url>
        </repository>
        <repository>
            <id>public</id>
            <url>http://maven.nuxeo.org/public</url>
            <releases>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>public-snapshot</id>
            <url>http://maven.nuxeo.org/public-snapshot</url>
            <releases>
                <enabled>false</enabled>
            </releases>
            <snapshots>
            </snapshots>
        </repository>


    </repositories>

